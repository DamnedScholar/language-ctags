'.emv':
  'IF':
    'prefix': 'emvif'
    'body': '[EMV IF]($1 = $2)[EMV THEN]$3[EMV /THEN][EMV /IF]'
  'ELSE':
    'prefix': 'emvelse'
    'body': '[EMV ELSE]$1[EMV /ELSE]'
  'DYN':
    'prefix': 'emvdyn'
    'body': '[EMV DYN]$1[EMV /DYN]'
  'CONTENT':
    'prefix': 'emvcontent'
    'body': '[EMV CONTENT]$1[EMV /CONTENT]'
  'INCLUDE':
    'prefix': 'emvinclude'
    'body': '[EMV INCLUDE]$1[EMV /INCLUDE]'
  'TEXTPART':
    'prefix': 'emvdyn'
    'body': '[EMV TEXTPART]$1[EMV /TEXTPART]'
